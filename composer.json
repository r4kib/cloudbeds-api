{
  "name": "r4kib/cloudbeds-api",
  "description": "cloudbeds.com API wrapper for PHP",
  "keywords": [
    "PHP",
    "cloudbeds.com",
    "cloudbeds",
    "API",
    "Wrapper"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "rakib hasan",
      "email": "rakib.hbo@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "league/oauth2-client": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "R4kib\\Cloudbeds\\": "src/"
    }
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.5"
  },
  "autoload-dev": {
    "psr-4": {
      "R4kib\\Cloudbeds\\Tests\\": "tests/src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "R4kib\\Cloudbeds\\CloudbedsServiceProvider"
      ]
    }
  }
}
